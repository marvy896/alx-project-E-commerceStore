{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "typeRoots": ["./node_modules/@types", "./src/@types"],
      "rootDirs": [".", "./build/@types"],
      "strict": true,
      "noUncheckedIndexedAccess": true,
      "lib": ["es2020", "DOM"],
      "target": "ES5",
      "resolveJsonModule": true,
    }
    ,
    "exclude"        : [
        "node_modules",
        "**/*spec.ts"
    ], "include"        : [
        "src/**/*"
, "nav.tsx"    ]

}